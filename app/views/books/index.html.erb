<p id="notice"></p>
<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <td>本のタイトル</td>
      <td>本の内容</td>
      <td>Show</td>
      <td>Edit</td>
      <td>Destroy</td>
  </tbody>
</table>
<h2>New book</h2>
<form>
  <div class="field">
    <label>Title</label>
    <input class="book_title" type="text" name="book[title]" id="book_title">
  </div>
  <div class="field">
    <label>Body</label>
    <textarea class="book_body" name="book[body]" id="book_body"></textarea>
  </div>
  <div class="actions">
    <input type="submit" name="commit" value="Create Book" date-disable-with="Create Book">
  </div>
</form>
